# MongoDB Atlas Configuration
# Get these from https://cloud.mongodb.com/v2#/org/{ORG-ID}/access/apiKeys
MONGODB_ATLAS_PUBLIC_KEY=your_public_api_key_here
MONGODB_ATLAS_PRIVATE_KEY=your_private_api_key_here
MONGODB_ATLAS_ORG_ID=your_organization_id_here

# AWS Configuration for Local Development (if not using OIDC locally)
# For local development, you can still use access keys
# AWS_ACCESS_KEY_ID=your_aws_access_key_here
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# Database Passwords (generate secure passwords)
DATABASE_PASSWORD=your_secure_admin_password_here
APP_PASSWORD=your_secure_app_password_here
READONLY_PASSWORD=your_secure_readonly_password_here

# Optional: Local development
TF_LOG=INFO
TF_LOG_PATH=./terraform.log